{
  "name": "@morphcode/collections",
  "version": "1.0.0",
  "description": "A comprehensive collection of data structures for JavaScript and TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "tsc",
    "build:dist": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts",
    "clean": "rm -rf dist lib coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepare": "husky install"
  },
  "keywords": [
    "data-structures",
    "collections",
    "algorithms",
    "typescript",
    "javascript",
    "library",
    "heap",
    "counter",
    "dict",
    "deque"
  ],
  "author": "Anibal Laura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morphcode-io/collections-js.git"
  },
  "bugs": {
    "url": "https://github.com/morphcode-io/collections-js/issues"
  },
  "homepage": "https://github.com/morphcode-io/collections-js#readme",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "husky": "^8.0.0",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "rollup": "^4.45.0",
    "rollup-plugin-dts": "^6.2.1",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.4.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
